  "output_tests": [
    ["-c 1 127.0.0.1"],  // Testa un singolo pacchetto ICMP verso localhost
    ["-c 4 8.8.8.8"],    // Testa 4 pacchetti verso il DNS pubblico di Google
    ["-n -c 2 www.google.com"],  // Testa la risoluzione DNS disabilitata, dovrebbe restituire solo l'IP
    ["-s 56 8.8.8.8"],   // Testa il payload di dimensione 56 byte
    ["-T tsonly -c 3 8.8.8.8"],  // Test con timestamp IP
    ["--ttl 64 8.8.8.8"],  // TTL di 64
    ["--ttl 1 8.8.8.8"],   // TTL di 1 (test per la verifica dell'hop limit)
    ["-f -c 100 127.0.0.1"], // Flooding con 100 pacchetti verso localhost
    ["-p ff00ff00 8.8.8.8"], // Pattern di payload specifico
    ["-r -c 2 8.8.8.8"],    // Ignora routing table
    ["-W 5 8.8.8.8"],    // Timeout di 5 secondi per l'attesa della risposta
    ["-l 10 127.0.0.1"],   // Testa la latenza con backlog di 10 pacchetti
    ["-T tsandaddr -c 3 8.8.8.8"], // Timestamp e indirizzi IP
    ["-n -f -c 50 127.0.0.1"], // Test di flood senza risoluzione DNS
    ["-v -c 3 8.8.8.8"],   // Verbose output, controlla maggiori dettagli nelle risposte
    ["-s 1200 8.8.8.8"],   // Dimensione pacchetto superiore a 1024 byte
    ["-w 10 8.8.8.8"],   // Test con un limite di attesa di 10 secondi
    ["-T prespecified -c 5 8.8.8.8"],  // Test con opzione di timestamp predefinito
    ["-f -c 1000 127.0.0.1"]  // Flood di 1000 pacchetti verso localhost
  ],

  "performance_tests": [
    ["-f -c 1000 127.0.0.1"],   // Testa il flooding di pacchetti per la performance
    ["-c 1000 8.8.8.8"],       // 1000 pacchetti consecutivi verso un server esterno
    ["-n -c 5000 8.8.8.8"],    // 5000 pacchetti senza risoluzione DNS
    ["-s 1024 -c 100 8.8.8.8"],  // Pacchetti di 1024 byte con 100 iterazioni
    ["-l 20 -c 100 8.8.8.8"],   // Testa backlog di 20 pacchetti
    ["-W 10 -c 5000 8.8.8.8"],  // 5000 pacchetti con timeout di 10 secondi
    ["-p abcdefabcdef -c 2000 8.8.8.8"], // 2000 pacchetti con pattern specifico
    ["-f -l 50 -c 1000 127.0.0.1"],  // Flood con backlog elevato
    ["-T tsonly -c 200 8.8.8.8"], // Testa il timestamp IP su 200 pacchetti
    ["-v -n -f -c 500 127.0.0.1"], // Testa il flooding con output verboso
    ["-r -s 1500 -c 500 8.8.8.8"],  // Pacchetti di grandi dimensioni (1500 byte)
    ["-T tsandaddr -c 1000 8.8.8.8"], // Testa con timestamp e indirizzo per 1000 pacchetti
    ["-W 1 -c 5000 127.0.0.1"] // Timeout di 1 secondo con 5000 pacchetti locali
  ]
